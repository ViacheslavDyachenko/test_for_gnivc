{
    "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "components": ["components"],
      "store": ["store"],
      "styles": ["styles"],
      "utils": ["utils"],
    },
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports" : true,
      "esModuleInterop" : true,
    }
}